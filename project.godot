; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[addons]

resources_spreadsheet_view/color_rows=true
resources_spreadsheet_view/color_arrays=true
resources_spreadsheet_view/array_min_width=128.0
resources_spreadsheet_view/resource_preview_size=32.0
resources_spreadsheet_view/clip_headers=false
resources_spreadsheet_view/dupe_arrays=true
resources_spreadsheet_view/context_menu_on_leftclick=false

[application]

config/name="BaseProject"
config/tags=PackedStringArray("template")
run/main_scene="res://game/ui/main_menu/MainMenu.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

ControllerIcons="*res://addons/controller_icons/ControllerIcons.gd"
SceneLoader="*res://addons/scene_loader/autoloads/scene_loader.gd"

[editor_plugins]

enabled=PackedStringArray("res://addons/AS2P/plugin.cfg", "res://addons/automatic_layer_enums/plugin.cfg", "res://addons/controller_icons/plugin.cfg", "res://addons/dictionary_inspector/plugin.cfg", "res://addons/godot-flexbox/plugin.cfg", "res://addons/godot-idea-board/plugin.cfg", "res://addons/resources_spreadsheet_view/plugin.cfg", "res://addons/scene_loader/plugin.cfg")

[godot_idea_board]

directory_path="res://addons/godot-idea-board/savedata/"

[rendering]

renderer/rendering_method.mobile="gl_compatibility"
